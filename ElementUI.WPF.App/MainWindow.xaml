<Window x:Class="ElementUI.WPF.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:eui="clr-namespace:ElementUI.WPF.Component;assembly=ElementUI.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <eui:Icon Grid.Row="0" Grid.Column="0" IconCode="&#xe606;" />
        <eui:Icon Grid.Row="0" Grid.Column="1" IconCode="&#xe617;" />
        <eui:Icon Grid.Row="0" Grid.Column="2" IconCode="&#xe64f;" />
        <eui:Icon Grid.Row="0" Grid.Column="3" IconCode="&#xe603;" />

        <eui:Icon Grid.Row="1" Grid.Column="0" IconCode="&#xe606;" />
        <eui:Icon Grid.Row="1" Grid.Column="1" IconCode="&#xe606;" />
        <eui:Icon Grid.Row="1" Grid.Column="2" IconCode="&#xe606;" />
        <eui:Icon Grid.Row="1" Grid.Column="3" IconCode="&#xe602;" />

        <eui:Icon Grid.Row="2" Grid.Column="0" IconCode="&#xe64c;" />
        <eui:Icon Grid.Row="2" Grid.Column="1" IconCode="&#xe61c;" />
        <eui:Icon Grid.Row="2" Grid.Column="2" IconCode="&#xe67a;" />
        <eui:Icon Grid.Row="2" Grid.Column="3" IconCode="&#xe608;" />

        <eui:Icon Grid.Row="3" Grid.Column="0" IconCode="&#xe697;" />
        <eui:Icon Grid.Row="3" Grid.Column="1" IconCode="&#xe60c;" />
        <eui:Icon Grid.Row="3" Grid.Column="2" IconCode="&#xe7e5;" />
        <eui:Icon Grid.Row="3" Grid.Column="3" IconCode="&#xe603;" />

    </Grid>

</Window>